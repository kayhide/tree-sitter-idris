{
  "grammars": [
    {
      "name": "idris",
      "camelcase": "Idris",
      "scope": "source.idris",
      "path": ".",
      "file-types": [
        "idr"
      ],
      "injection-regex": "^idris$"
    }
  ],
  "metadata": {
    "version": "0.0.1",
    "license": "MIT",
    "description": "Idris grammar for tree-sitter",
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-idris"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
